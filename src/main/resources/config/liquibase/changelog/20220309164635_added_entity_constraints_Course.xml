        <addForeignKeyConstraint baseColumnNames="prof_id"
                                 baseTableName="course_prof"
                                 constraintName="fk_course_prof_prof_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
